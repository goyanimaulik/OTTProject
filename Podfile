# platform :ios, '9.0'

target 'OTTProject' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OTTProject
  pod 'FLAnimatedImage', '~> 1.0'
  pod 'UIScrollView-InfiniteScroll'
  
