    pod 'UIScrollView-InfiniteScroll'
  end

  target 'OTTProjectUITests' do
    # Pods for testing
    pod 'FLAnimatedImage', '~> 1.0'
    pod 'UIScrollView-InfiniteScroll'
  end

end
